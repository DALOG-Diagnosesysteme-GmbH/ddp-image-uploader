{
  "AuthSettings": {
    "ClientId": "d55fae3d-79ce-43ce-b09a-151b50255d5c",
    "RedirectUrl": "https://dalogddpb2cprd.b2clogin.com/oauth2/nativeclient",
    "B2CTenantName": "dalogddpb2cprd",
    "B2CPolicyName": "B2C_1_susi"
  },
  "AppSettings": {
    "BaseUrl": "https://api.dalog.net/",
    "PingUrl": "files/v1/ping",
    "AutoUploadIntervalHours": 2
  },
  "ImagesUploadEndpoints": {
    "Default": "files/v1/images",
    "BusyBee": "files/v1/images/busybee",
    "FLS": "files/v1/images/fls",
    "Zip": "files/v1/images/zip",
    "GZip": "files/v1/images/gzip",
    "Wireless": "files/v1/images/wireless"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Expressions" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:/DALOG/ImagesUploader/log.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 10,
          "formatter": {
            "type": "Serilog.Templates.ExpressionTemplate, Serilog.Expressions",
            "template": "[{@t:yyyy.MM.dd HH:mm:ss} {@l:u3} {#if SourceContext is not null}({Substring(SourceContext, LastIndexOf(SourceContext, '.') + 1)}){#end}] {@m}\n{@x}"
          }
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "Properties": {
      "ApplicationName": "DALOG Images Uploader"
    }
  }
}
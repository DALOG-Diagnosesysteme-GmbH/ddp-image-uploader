{
  "AppSettings": {
    "BaseUrl": "https://api.dalog.net/dev/",
    "PingUrl": "files/v1/ping",
    "AutoUploadIntervalHours": 2
  },
  "ImagesUploadEndpoints": {
    "Default": "files/v1/images",
    "BusyBee": "files/v1/images/busybee",
    "FLS": "files/v1/images/fls",
    "Zip": "files/v1/images/zip",
    "GZip": "files/v1/images/gzip",
    "Wireless": "files/v1/images/wireless"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Debug", "Serilog.Expressions" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Debug",
        "Args": {
          "formatter": {
            "type": "Serilog.Templates.ExpressionTemplate, Serilog.Expressions",
            "template": "[{@t:yyyy.MM.dd HH:mm:ss} {@l:u3} {#if SourceContext is not null}({Substring(SourceContext, LastIndexOf(SourceContext, '.') + 1)}){#end}] {@m}\n{@x}"
          }
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithExceptionDetails"
    ],
    "Properties": {
      "ApplicationName": "VPN Connections Backup"
    }
  }
}